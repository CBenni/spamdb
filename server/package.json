{
  "name": "spamdb-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*' -e ts,tsx --exec 'ts-node' ./src/index.ts",
    "generate:routes:v1": "tsoa routes -c src/api/v1/tsoa.json",
    "generate:swagger:v1": "tsoa swagger -c src/api/v1/tsoa.json",
    "generate:routes": "npm run generate:routes:v1",
    "generate:swagger": "npm run generate:swagger:v1",
    "generate:all": "npm run generate:routes && npm run generate:swagger",
    "generate": "npm run generate:all"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "got": "^8.3.0",
    "koa": "^2.5.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.10",
    "pino": "^4.13.0",
    "socket.io": "^2.0.4",
    "tsoa": "^2.1.4",
    "typegoose": "^4.0.1",
    "uuid": "^3.2.1",
    "uws": "^9.14.0"
  },
  "devDependencies": {
    "@types/koa": "^2.0.44",
    "@types/koa-router": "^7.0.27",
    "@types/lodash": "^4.14.104",
    "@types/mongoose": "^5.0.7",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "nodemon": "^1.17.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.7.0",
    "typescript": "^2.7.2"
  }
}
